{
    "manifest_version": 2,
    "name": "YouAd",
    "version": "1.0.0",

    "description": "Sometime you need to YouAd. Othertimes you need to YouSubtract.",
    "content_scripts": [
        {
            "matches": [
                "https://youtube.com/*",
                "https://www.youtube.com/*"
            ],
            "js": [
                "js/content-script.js"
            ],
            "run_at": "document_end",
            "all_frames": false
        }
    ],
    "permissions": [
        "tabs",
        "history",
        "management",
        "https://youtube.com/*",
        "https://www.youtube.com/*"
    ],
    "web_accessible_resources": [
        "assets/*"
    ]
}
